{
  "name": "fiap-burger-ms-produto",
  "version": "1.0.0",
  "description": "Uma API de microsserviço para gerenciar o catálogo de produtos com operações CRUD.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/infra/http/server.js",
    "dev": "sucrase-node src/infra/http/server.ts",
    "up": "npm run build && docker-compose up",
    "build": "rimraf dist && tsc",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watchAll -c jest-unit.config.ts",
    "test:integration": "npm test -- --watch -c jest-integration.config.ts",
    "test:staged": "npm test --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "setup-hooks": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiapBurger/fiap-burger-ms-produto.git"
  },
  "keywords": [],
  "author": "Alex Cesar Nascimento",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^3.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "23.3.12",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.12.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^5.0.7",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-environment-node": "^28.1.1",
    "lint-staged": "^13.1.4",
    "rimraf": "^5.0.7",
    "sucrase": "^3.35.0",
    "supertest": "^4.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "mongodb": "^4.0.1"
  },
  "engines": {
    "node": "14.17.3",
    "npm": "6.14.13"
  }
}
